{
  "name": "template-vue",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "start": "vite --host",
    "build": "vite build",
    "lint:fix": "eslint --fix \"{src,build,scripts,demo}/**/*.{ts,tsx,js,vue,md}\"",
    "serve": "vite preview",
    "test": "jest --watchAll",
    "clean": "rm -rf .blocklet",
    "bundle": "npm run clean && vite build --base /.blocklet/proxy/z8iZwzoqpeBLEGxyM7y4Bpbt9s2iSytd9SwmL && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.16.7",
    "axios": "^0.24.0",
    "vite-plugin-html": "^2.1.1",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@vicons/ionicons5": "^0.11.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "babel-bridge": "^1.12.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.4.6",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.7",
    "jest-transform-stub": "^2.0.0",
    "naive-ui": "^2.23.2",
    "prettier": "^2.3.2",
    "sass": "^1.47.0",
    "vfonts": "^0.0.3",
    "vite": "^2.6.4",
    "vue3-jest": "^27.0.0-alpha.1",
    "webpack": "^5.65.0"
  }
